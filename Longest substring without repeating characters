class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        d={}
        max_len=0
        start=0
        for i,ch in enumerate(s):
            if ch in d and d[ch]>=start:
                start=d[ch]+1
            d[ch]=i
            max_len=max(max_len,i-start+1)
        return max_len
        
