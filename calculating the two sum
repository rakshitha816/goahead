def twoSum(nums,target):
    n=len(nums)
    low=0
    high=n-1
    while(low<high):
        Sum=nums[low]+nums[high]
        if(Sum==target):
            return "Yes"
            print(sum)
        elif(Sum>target):
            high-=1
        elif(Sum<target):
            low+=1
    return "No"
nums=list(map(int,input().split()))
target=int(input())
print(twoSum(nums,target))
